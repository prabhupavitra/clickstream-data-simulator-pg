{
  "SankeyDiagramVisual": {
      "VisualId": "{{visualId}}",
      "Title": {
          "FormatText": {
            "PlainText": "{{{title}}}"
          },
          "Visibility": "VISIBLE"
      },
      "Subtitle": {
          "FormatText": {
            "PlainText": "{{{subTitle}}}"
          },
          "Visibility": "VISIBLE"
      },
      "ChartConfiguration": {
          "FieldWells": {
              "SankeyDiagramAggregatedFieldWells": {
                  "Source": [
                      {
                          "CategoricalDimensionField": {
                              "FieldId": "{{sourceFieldId}}",
                              "Column": {
                                  "DataSetIdentifier": "{{dataSetIdentifier}}",
                                  "ColumnName": "source"
                              }
                          }
                      }
                  ],
                  "Destination": [
                      {
                          "CategoricalDimensionField": {
                              "FieldId": "{{targetFieldId}}",
                              "Column": {
                                  "DataSetIdentifier": "{{dataSetIdentifier}}",
                                  "ColumnName": "target"
                              }
                          }
                      }
                  ],
                  "Weight": [
                      {
                        "CategoricalMeasureField":{
                            "FieldId":"{{weightFieldId}}",
                            "Column":{
                                "DataSetIdentifier":"{{dataSetIdentifier}}",
                                "ColumnName":"x_id"
                            },
                            "AggregationFunction":"DISTINCT_COUNT"
                        }
                    }
                  ]
              }
          },
          "SortConfiguration": {
              "WeightSort": [
                  {
                      "FieldSort": {
                          "FieldId": "{{weightFieldId}}",
                          "Direction": "DESC"
                      }
                  }
              ]
          },
          "DataLabels": {
              "Visibility": "VISIBLE",
              "Overlap": "DISABLE_OVERLAP"
          }
      },
      "Actions": []
  }
}